QMainWindow {
	background-image: url(":/spongebob.jpg");
	background-repeat: no-repeat;
	background-position: center;
}

QPushButton {
	background-color: #FFD700;
	color: #000080;
	border: 2px solid #000080;
	border-radius: 10px;
	font-size: 14px;
	padding: 5px;
}

QPushButton:hover {
	background-color: #FFA500;
	color: white;
}

QPushButton:disabled {
	background-color: rgba(127, 127, 127, 180);
	color: #BBBBBB;
}

QLabel {
	color: #FFFF00;
	font-size: 16px;
	font-weight: bold;
}
